#!/usr/bin/env bash

# Iterate through files in the current directory
for file in *
do
    # Check if the file is a regular file (not a directory) and contains a dash in its name
    if [ -f "$file" ] && [[ "$file" == *-* ]]
    then
        # Extract part of the name after the first dash
        name=$(echo "$file" | cut -d'-' -f2-)
        echo "$name"
    fi
done
